<div class="container">
  <h1>Welcome to Awesome Blog!</h1>

  <a href='/post/new'>MAKE A POST</a>

  <h2>List of Posts</h2>
  <table>
   <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><a href="/post/<%= post.id %>"><%= post.title %></a></td>
      </tr>
    <% end %>
   </tbody>
  </table>
</div>
